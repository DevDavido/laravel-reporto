{
    "name": "devdavido/laravel-reporto",
    "description": "Log browser errors to the server using W3C Reporting API",
    "keywords": [
        "laravel-reporto",
        "laravel",
        "report-api",
        "browser",
        "frontend",
        "csp",
        "deprecations",
        "browser-interventions",
        "feature-policy",
        "network-error",
        "crash-report"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "DevDavido",
            "homepage": "https://github.com/DevDavido"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "laravel/framework": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit" : "^10.0",
        "scrutinizer/ocular": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "DevDavido\\Reporto\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DevDavido\\Reporto\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "DevDavido\\Reporto\\ReportoServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
